source 'https://github.com/CocoaPods/Specs.git'
platform :ios, ‘11.0’
use_frameworks!

target 'JiaKeNengOC' do
  #网络库
  pod 'AFNetworking', '~> 4.0.1'
  #列表下拉刷新
  pod 'MJRefresh', '~> 3.5.0'
  #字典转模型
  pod 'MJExtension', '~> 3.2.1'
  #列表空数据展示
  pod 'DZNEmptyDataSet', '~> 1.8.1'
  #计时器
  pod 'MSWeakTimer', '~> 1.1.0'
  #列表悬浮表头
  pod 'JXPagingView/Pager', '~> 2.0.6'
  pod 'JXCategoryView', '~> 1.5.5'
  #空安全运算符
  pod 'NullSafe', '~> 2.0'
  #腾讯UI框架
  pod 'QMUIKit', '~> 4.4.0'
  #图片缓存
  pod 'SDWebImage', '~> 5.11.0'
  #键盘管理
  pod 'IQKeyboardManager', '~> 6.5.6'
  #自动布局
  pod 'Masonry', '~> 1.1.0'
  #N多种类型的弹出选择器
  pod "WMZDialog", '~> 1.0.6'
  #gzip解压
  pod 'GZIP', '~> 1.3.0'

  #Lookin
  pod 'LookinServer', :configurations => ['Debug']

  pod 'YYCategories', '~> 1.0.4'
end
